<?xml version="1.0" encoding="UTF-8"?>
<videoAd>
	
	<!-- OPTIONAL expects a list of comma delimited video keys that should autoplay, remove or set to FALSE to disable -->
	<autoplayKeys>VIDEO_SAMSONITE_1,VIDEO_KENMORE_1</autoplayKeys>
	
	<!-- OPTIONAL number of times to loop the autoplay videos, default is 0 -->
	<!-- If the videos are set to loop 1 time, they will play through 2 times -->
	<autoplayLoop>1</autoplayLoop>
	
	<!-- OPTIONAL expects true or false, default is false -->
	<muteAutoplay>true</muteAutoplay>
	
	<!-- OPTIONAL expects true or false, default is false -->
	<restartVideoOnPlayAudioClick>true</restartVideoOnPlayAudioClick>
	
	<!-- OPTIONAL expects a number (in seconds), default is 3 seconds -->
	<bufferTime>3</bufferTime>
	
	<!-- OPTIONAL list of tracking nodes is optional, nothing tracked by default -->
	<!-- The player will track CONFIG_LOAD, PLAY_AUDIO, REPLAY if they exist. -->
	<!-- Any others added here can be called separately at any time. -->
	<tracks>
		<track>
			<key>CONFIG_LOAD</key>
			<path>http://www.timmcleod.com/?videotrack=configload&amp;stamp=[timestamp]</path>
		</track>
		<track>
			<key>PLAY_AUDIO</key>
			<path>http://www.timmcleod.com/?videotrack=playaudio&amp;stamp=[timestamp]</path>
		</track>
		<track>
			<key>REPLAY</key>
			<path>http://www.timmcleod.com/?videotrack=replay&amp;stamp=[timestamp]</path>
		</track>
		<track>
			<key>EXAMPLE_OF_SOME_OTHER_RANDOM_TRACKING</key>
			<path>http://www.timmcleod.com/?videotrack=exampletracking&amp;stamp=[timestamp]</path>
		</track>
		<track>
			<key>ANOTHER_EXAMPLE</key>
			<path>http://www.timmcleod.com/?videotrack=exampletracking&amp;stamp=[timestamp]</path>
		</track>
	</tracks>
	
	<!-- REQUIRED requires at least one video node -->
	<videos>
		<video>
		
			<!-- REQUIRED used to look up this video from list of videos -->
			<key>VIDEO_SAMSONITE_1</key>
			
			<!-- REQUIRED used to tell the player where to connect -->
			<path>rtmp://flash.scrippsnetworks.com/ondemand/NetworkAd/samsonite/</path>
			
			<!-- REQUIRED name of the video (without the file ext) -->
			<name>samsonite-bullfightshort</name>
			
			<!-- OPTIONAL when video ends, player will show a frame at this time -->
			<!-- Value is in seconds, set to -1 to disable. Disabled by default. -->
			<stopFrameTime>15</stopFrameTime>
			
			<!-- OPTIONAL tracking for videos, nothing tracked by default -->
			<tracks>
				<track>
					<!-- REQUIRED when vid reaches this time, tracking fires -->
					<time>0</time>
					<path>http://www.timmcleod.com/?video1track=startvideo&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<time>2</time>
					<path>http://www.timmcleod.com/?video1track=2&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<time>4</time>
					<path>http://www.timmcleod.com/?video1track=4&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<time>15</time>
					<path>http://www.timmcleod.com/?video1track=15&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<key>PLAY_AUDIO</key>
					<path>http://www.timmcleod.com/?videotrack=PLAY_AUDIO_VIDEO_SAMSONITE_1&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<key>REPLAY</key>
					<path>http://www.timmcleod.com/?videotrack=REPLAY_VIDEO_SAMSONITE_1&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<key>EXAMPLE_OF_SOME_OTHER_RANDOM_TRACKING</key>
					<path>http://www.timmcleod.com/?videotrack=EXAMPLE_OF_SOME_OTHER_RANDOM_TRACKING_VIDEO_SAMSONITE_1&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<key>ANOTHER_EXAMPLE</key>
					<path>http://www.timmcleod.com/?videotrack=ANOTHER_EXAMPLE_TRACKING_VIDEO_SAMSONITE_1&amp;stamp=[timestamp]</path>
				</track>
			</tracks>
		</video>
		<video>
		
			<!-- REQUIRED used to look up this video from list of videos -->
			<key>VIDEO_SAMSONITE_2</key>
			
			<!-- REQUIRED used to tell the player where to connect -->
			<path>rtmp://flash.scrippsnetworks.com/ondemand/NetworkAd/samsonite/</path>
			
			<!-- REQUIRED name of the video (without the file ext) -->
			<name>samsonite-bullfightshort</name>
			
			<!-- OPTIONAL when video ends, player will show a frame at this time -->
			<!-- Value is in seconds, set to -1 to disable. Disabled by default. -->
			<stopFrameTime>15</stopFrameTime>
			
		</video>
		<video>
			<key>VIDEO_KENMORE_1</key>
			<path>rtmp://flash.scrippsnetworks.com/ondemand/SponsorContent/kenmore/</path>
			<name>0153640</name>
			<stopFrameTime>2</stopFrameTime>
			<tracks>
				<track>
					<time>0</time>
					<path>http://www.timmcleod.com/?video3track=startvideo&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<time>2</time>
					<path>http://www.timmcleod.com/?video3track=2&amp;stamp=[timestamp]</path>
				</track>
				<track>
					<time>4</time>
					<path>http://www.timmcleod.com/?video3track=4&amp;stamp=[timestamp]</path>
				</track>
			</tracks>
		</video>
		<video>
			<key>VIDEO_KENMORE_2</key>
			<path>rtmp://flash.scrippsnetworks.com/ondemand/SponsorContent/kenmore/</path>
			<name>0153641</name>
		</video>
	</videos>
</videoAd>